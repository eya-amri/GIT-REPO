pipeline {
    agent any
    tools{
        jdk 'JDK21'
    }

    stages {
        stage('checkout code') {
            steps {
                git branch: 'master', url: 'https://github.com/eya-amri/GIT-REPO.git'
            }
        }
        stage('compile code'){
            steps{
                sh 'javac src/application/Test.java'
            }
        }
        stage('excute code'){
            steps{
                sh 'java -cp src application.Test'
            }
        }
            
        
    }
